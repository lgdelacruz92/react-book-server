{
  "extends": "@tsconfig/node16/tsconfig.json",
  "ts-node": {
    "extends": "@tsconfig/node16/tsconfig.json",
    "require": ["tsconfig-paths/register"],
    "compilerOptions": {
      "target": "ES2021",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "incremental": true,
      "module": "CommonJS",
      "paths": {
        "@/*": ["./src/*"]
      },
 
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
  },
    "compilerOptions": {
      "target": "es5",
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "incremental": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      },
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
  }
  